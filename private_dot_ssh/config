ServerAliveInterval 300
Include ~/.ssh/config.private

host homepage
    hostname wersdoerfer.de
    ForwardAgent yes
    User homepage

host python-podcast
    hostname wersdoerfer.de
    ForwardAgent yes
    User python-podcast

host macmini-unlock
    hostname 192.168.178.94
    User root
    Port 2222
    IdentityFile ~/.ssh/unlock-macmini
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

host fractal-unlock
    hostname 192.168.178.116
    User root
    Port 2222
    IdentityFile ~/.ssh/unlock-macmini
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

host code.egenix.com
    hostname code.egenix.com
    User git
    Port 2242
    IdentityFile ~/.ssh/egenix_gitlab
    IdentitiesOnly yes
